    {
      "parameters": {
        "functionCode": "// Conduct CMA - Comparative Market Analysis\n// In real scenario, this would call MLS API or real estate data API\n\nconst property = items[0].json;\nconst sqft = parseInt(property.square_feet);\nconst desiredPrice = parseFloat(property.desired_price);\n\n// Simulate market analysis\nconst pricePerSqft = 250; // Average in area\nconst marketValue = sqft * pricePerSqft;\nconst recommendedPrice = Math.round(marketValue * 0.95); // Slight discount\n\nconst comparables = [\n  { address: '123 Main St', price: 450000, sqft: 1800, beds: 3, baths: 2 },\n  { address: '456 Oak Ave', price: 475000, sqft: 1900, beds: 3, baths: 2.5 },\n  { address: '789 Elm St', price: 425000, sqft: 1700, beds: 3, baths: 2 }\n];\n\nreturn {\n  json: {\n    ...property,\n    cma_completed: true,\n    market_value: marketValue,\n    recommended_price: recommendedPrice,\n    comparables: comparables,\n    price_variance: ((recommendedPrice - desiredPrice) / desiredPrice * 100).toFixed(2)\n  }\n};"
      },
      "name": "Conduct CMA (Market Analysis)",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [1650, 300],
      "id": "conduct-cma"
    },
